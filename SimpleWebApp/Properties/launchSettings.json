    "anonymousAuthentication": true, //allow anonymous access to application
    "iisExpress": {
      "applicationUrl": "http://localhost:64940",
      "sslPort": 44315
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
